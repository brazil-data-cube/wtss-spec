{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://www.esensing.dpi.inpe.br/wtss/list_coverages_response.json",
  "type": "object",
  "title": "WTSS - ListCoverages operation",
  "description": "Retrieves list of offered coverages",
  "required": [
    "coverages"
  ],
  "properties": {
    "coverages": {
      "$id": "#/properties/coverages",
      "type": "array",
      "title": "Coverages Names",
      "description": "Defines list of coverage names from server",
      "default": false,
      "items": {
        "$id": "#/properties/coverages/items",
        "type": "string",
        "title": "Coverage Identifier",
        "description": "Coverage identifier",
        "default": "",
        "examples": [
          "MOD13Q1"
        ]
      }
    }
  }
}